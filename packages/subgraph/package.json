{
  "name": "@astralprotocol/subgraph",
  "version": "0.0.1",
  "description": "Subg",
  "author": "Astral <contact@astral.global>",
  "homepage": "https://github.com/AstralProtocol/astralprotocol#readme",
  "license": "ISC",
  "main": "lib/mapping.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AstralProtocol/astralprotocol.git",
    "directory": "packages/subgraph"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "graph-node": "sudo rm -rf graph-node/docker/data/postgres && docker-compose -f graph-node/docker/docker-compose.yml up",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ astraldao/spatial-registry-v02 --access-token .token",
    "create-local": "graph create --node http://localhost:8020/ astraldao/spatial-assets-nfts",
    "remove-local": "graph remove --node http://localhost:8020/ astraldao/spatial-assets-nfts",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 astraldao/spatial-assets-nfts",
    "remove-subgraph": "graph remove astraldao/spatial-assets --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "clean": "rm -rf ./lib ./tsconfig.tsbuildinfo"
  },
  "bugs": {
    "url": "https://github.com/AstralProtocol/astralprotocol/issues"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.19.0",
    "@graphprotocol/graph-ts": "^0.19.0"
  }
}
