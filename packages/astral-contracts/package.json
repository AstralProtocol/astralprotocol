{
  "name": "@astralprotocol/astral-contracts",
  "version": "0.0.2",
  "description": "> TODO: description",
  "author": "Jared Childers <jared.childers97@gmail.com>",
  "homepage": "https://github.com/astralDAO/astralprotocol#readme",
  "license": "ISC",
  "main": "lib/astral-contracts.js",
  "typings": "lib/astral-contracts.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astralDAO/astralprotocol.git",
    "directory": "packages/astral-contracts"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "console": "truffle console --network development",
    "ganache": "ganache-cli -h 0.0.0.0 -p 8545 -d -b 3 -i 333",
    "graph-node": "sudo rm -rf graph-node/docker/data/postgres && docker-compose -f graph-node/docker/docker-compose.yml up",
    "truffle": "truffle compile --all && truffle migrate --network development --reset",
    "create": "graph create p2pchain/subgraph --node https://api.thegraph.com/deploy/",
    "create-local": "graph create p2pchain/subgraph --node http://127.0.0.1:8020",
    "create-gcloud": "graph create p2pchain/subgraph --node https://p2pchain.ml/create",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy p2pchain/subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy p2pchain/subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy-gcloud": "graph deploy p2pchain/subgraph --ipfs https://p2pchain.ml/deploy/ --node https://p2pchain.ml/create",
    "remove-subgraph": "graph remove p2pchain/subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "remove-subgraph-gcloud": "graph remove p2pchain/subgraph --ipfs https://p2pchain.ml/deploy --node https://p2pchain.ml/create",
    "mythx": "truffle run verify",
    "slither": "slither .",
    "truffle-test": "truffle test",
    "coverage": "truffle run coverage"
  },
  "bugs": {
    "url": "https://github.com/astralDAO/astralprotocol/issues"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.2.0",
    "@truffle/hdwallet-provider": "^1.0.44",
    "truffle": "^5.1.44"
  }
}
